[VARIABLES]

[INPUT]
	MAIN $|$ END
	SUBROUTINE$|$ RETURN
	FUNCTION$|$ RETURN 1

[EVENT]
	MSGBOX   "Hello World"
	INPUT
	LABEL	
	READ
	KEYHIT
	LEFT_BUTTON
	RIGHT_BUTTON
	GET_MESSAGE

[PROCESS]
	%1
	CALL 
	MPLAY
	RUN
	INSECTA
	OPEN
	CLOSE
	MOVE
	RESIZE
	SHOW
	HIDE
	DLL 
	DLL hWnd
	INSTALL PROGRAM
	INSTALL LIBS
	INSTALL GENERATOR
	INSTALL TEMPLATE

[SET]
	--
	++
	= YESNO
	= DLL
	= DLL hWnd
	= HWND
	= FUNCTION
	= EXISTS

[OUTPUT]
	BEEP
	PRINT
	WRITE
	SEND_MESSAGE
	POST_MESSAGE
	EXIT_INSECTA
	#include 

[LOOP]
	?
	?  	$|$
	<  ?
	>  ?
	<= ?
	>= ?
	=  ?
	<> ?
	-- ?
	++ ?
	!= ?
	++
	--
	= DLL
	= DLL hWnd
	= FUNCTION
	= YESNO

[LEND]
	?
	<  ?
	>  ?
	<= ?
	>= ?
	=  ?
	-- ?
	++ ?
	!= ?
	++
	--
	= DLL
	= DLL hWnd
	= FUNCTION
	= YESNO

[BRANCH]
	?  $|$ TRUE $|$ FALSE $|$
	<  ?
	>  ?
	<= ?
	>= ?
	=  ?
	-- ?
	++ ?
	!= ?
	YESNO $|$ TRUE $|$ FALSE $|$

[PATH]
	TRUE
	FALSE
	DEFUALT

[BEND]
	ENDIF
	= DLL
	= DLL hWnd
	= FUNCTION
	--
	++

[END]
	BREAK
	RETURN
	RETURN TRUE
	RETURN FALSE
	EXIT
	END
	GOTO

[RUN_BUTTON]

[HELP_BUTTON]
	IRL.hlp 				|Insecta Robotica Language

[TOOLS]
	C:\WINDOWS\COMMAND.COM   PROMPT Insecta:	|DOS Machine		
					SEPARATOR
	C:\WINDOWS\SYSMON.EXE			|System Monitor
	C:\WINDOWS\RSRCMTR.EXE			|Resource Meter					

[HELP]
	IRL.hlp 				|Insecta Robotica Language

